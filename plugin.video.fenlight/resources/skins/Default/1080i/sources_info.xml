                    <width max="1275">auto</width>
                    <height>20</height>
                    <font>font14</font> <!-- FENLIGHT_33 -->
                    <textcolor>FFCCCCCC</textcolor>
                    <align>left</align>
                    <aligny>center</aligny>
                    <label>[UPPERCASE][COLOR $INFO[Window.Property(highlight)]][B]Info: [/B][/COLOR]$INFO[Window.Property(extraInfo)][/UPPERCASE]</label>
                </control>
                <control type="textbox">
                    <top>340</top>
                    <left>0</left>
                    <width max="1275">auto</width>
                    <height>140</height>
                    <font>font14</font> <!-- FENLIGHT_33 -->
                    <textcolor>FFCCCCCC</textcolor>
                    <align>left</align>
                    <aligny>top</aligny>
                    <label>[UPPERCASE][COLOR $INFO[Window.Property(highlight)]][B]Title: [/B][/COLOR]$INFO[Window.Property(name)][/UPPERCASE]</label>
                </control>
            </control>
        </control>
    </controls>
</window>
